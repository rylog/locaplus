{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "locaplus.net" }],
      "destination": "https://www.chapiteaulocaplus.net/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.locaplus.net" }],
      "destination": "https://www.chapiteaulocaplus.net/$1",
      "permanent": true
    },

    {
      "source": "/index.php/fr/:path*",
      "destination": "/fr",
      "permanent": true
    },
    {
      "source": "/index.php/en/:path*",
      "destination": "/en",
      "permanent": true
    }
  ]
}
