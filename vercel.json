{

  "rewrites":  [
    {"source": "/(.*)", "destination": "/index.html"}
  ],

"redirects": [
    {
      "source": "/frdex.php/en",
      "destination": "/en",
      "permanent": true
    },
    {
      "source": "/frdex.php/fr",
      "destination": "/fr",
      "permanent": true
    }
  ],

  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control", "value": "max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
